{
  "name": "react-training",
  "version": "1.0.0",
  "description": "React Training, step by step",
  "main": "index.js",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-actions": "^0.8.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^6.4.0",
    "browserify": "^12.0.1",
    "live-server": "^0.8.2",
    "livereactload": "^2.1.0",
    "npm-run-all": "^1.3.2",
    "react-proxy": "^1.1.1",
    "serve": "^1.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "build": "npm-run-all 'build:*'",
    "build:js": "browserify -t babelify src/app.js > static/app.js",
    "watch": "npm-run-all --parallel 'watch:*'",
    "watch:js": "watchify -v -p livereactload -t babelify --debug src/app.js -o static/app.js",
    "watch:server": "live-server --port=3000 --ignore=app.js static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmtm/react-training.git"
  },
  "author": "ByteClub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmtm/react-training/issues"
  },
  "homepage": "https://github.com/lmtm/react-training#readme"
}
